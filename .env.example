# Environment Variables Template for NutriSnap Backend
# Copy this file to .env and fill in your actual API keys

# Google Gemini API Key (Vision Agent)
# Obtain from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_google_gemini_api_key_here

# Groq API Key (Advisor Agent - Llama 3)
# Obtain from: https://console.groq.com/keys
GROQ_API_KEY=your_groq_api_key_here

# CalorieNinjas API Key (Nutrition Quantification)
# Obtain from: https://calorieninjas.com/api
CALORIENINJAS_API_KEY=your_calorieninjas_api_key_here

# MongoDB Connection URI
# Default for local development
MONGO_URI=mongodb://localhost:27017

# Database Name
DATABASE_NAME=nutrisnap_db
